{
  "name": "@groton/knowledgebase.router",
  "version": "0.1.0",
  "homepage": "https://github.com/groton-school/knowledgebase/tree/main/apps/router#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/groton-school/knowledgebase.git",
    "directory": "apps/router"
  },
  "author": {
    "name": "Seth Battis",
    "email": "sbattis@groton.org"
  },
  "type": "module",
  "scripts": {
    "build": "run-s build:*",
    "build:sync-config": "source .env && tsx ./bin/sync-config.ts -c $CONFIG",
    "build:compile": "tsc",
    "build:bundle": "webpack",
    "build:cleanup": "del ./tmp_tsc",
    "deploy": "run-s deploy:* || tsx ./bin/restore-lockfile.ts",
    "deploy:build": "run-s build",
    "deploy:prepare-lockfile": "tsx ./bin/prepare-lockfile.ts",
    "deploy:deploy": "tsx bin/deploy.ts --commands --silent",
    "deploy:restore-lockfile": "tsx ./bin/restore-lockfile.ts",
    "deploy:unbork-pnpm": "pnpm i -rf",
    "start": "node build/main.cjs"
  },
  "dependencies": {
    "@google-cloud/connect-firestore": "^3.0.0",
    "@google-cloud/firestore": "^7.10.0",
    "@google-cloud/logging-winston": "^6.0.0",
    "@google-cloud/storage": "^7.14.0",
    "@googleapis/drive": "^8.14.0",
    "@groton/knowledgebase.api": "workspace:*",
    "@groton/knowledgebase.config": "workspace:*",
    "@groton/knowledgebase.indexer": "workspace:*",
    "@groton/knowledgebase.openid": "workspace:*",
    "@groton/knowledgebase.ui": "workspace:*",
    "express": "^4.21.1",
    "express-session": "^1.18.1",
    "google-auth-library": "^9.15.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@battis/partly-gcloudy": "^0.6.0",
    "@battis/qui-cli": "^0.6.3",
    "@pnpm/make-dedicated-lockfile": "^0.6.20",
    "@tsconfig/node20": "^20.1.4",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/node": "^20.17.7",
    "copy-webpack-plugin": "^12.0.2",
    "del-cli": "^6.0.0",
    "npm-run-all": "^4.1.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "webpack": "5.95.0",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0",
    "yaml": "^2.6.1"
  }
}
