{
  "name": "@groton/knowledgebase.router",
  "version": "0.1.0",
  "author": {
    "name": "Seth Battis",
    "email": "sbattis@groton.org"
  },
  "scripts": {
    "setup": "bin/setup.sh",
    "build": "run-s build:*",
    "build:ui": "node -e \"if (! require('fs').existsSync('../ui/build')) { process.exit(1)}\" || pnpm -F @groton/knowledgebase.ui run build",
    "build:rollup": "rollup -c rollup.config.mjs",
    "deploy": "run-s deploy:* || bin/restore-lockfile.ts",
    "deploy:prepare-lockfile": "bin/prepare-lockfile.ts",
    "deploy:deploy": "bin/deploy.ts",
    "deploy:restore-lockfile": "bin/restore-lockfile.ts",
    "start": "node build/index.js"
  },
  "devDependencies": {
    "@battis/partly-gcloudy": "^0.4.4",
    "@battis/qui-cli": "^0.4.2",
    "@groton/knowledgebase.api": "workspace:*",
    "@groton/knowledgebase.config": "workspace:*",
    "@groton/knowledgebase.index": "workspace:*",
    "@groton/knowledgebase.openid": "workspace:*",
    "@pnpm/make-dedicated-lockfile": "^0.6.12",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@tsconfig/recommended": "^1.0.7",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^4.19.0",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-node-externals": "^7.1.2",
    "tslib": "^2.6.3",
    "tsx": "^4.16.2",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "@google-cloud/connect-firestore": "^3.0.0",
    "@google-cloud/firestore": "^7.9.0",
    "@google-cloud/logging-winston": "^6.0.0",
    "@google-cloud/storage": "^7.12.0",
    "@googleapis/drive": "^8.11.0",
    "express": "^4.19.2",
    "express-session": "^1.18.0",
    "google-auth-library": "^9.11.0",
    "winston": "^3.13.1"
  }
}
