{
  "name": "@groton/knowledgebase.router",
  "version": "0.1.0",
  "homepage": "https://github.com/groton-school/knowledgebase/tree/main/apps/router#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/groton-school/knowledgebase.git",
    "directory": "apps/router"
  },
  "author": {
    "name": "Seth Battis",
    "email": "sbattis@groton.org"
  },
  "scripts": {
    "build": "run-s build:*",
    "build:sync-config": "tsx bin/sync-config.ts -c ../../config.json",
    "build:index": "node -e \"if (! require('fs').existsSync('var/index.json') || ! require('fs').existsSync('var/groups.json')) { process.exit(1)}\" || pnpm -F @groton/knowledgebase.index run upload",
    "build:ui": "node -e \"if (! require('fs').existsSync('../ui/build')) { process.exit(1)}\" || pnpm -F @groton/knowledgebase.ui run build",
    "build:compile": "webpack",
    "deploy": "run-s deploy:* || tsx bin/restore-lockfile.ts",
    "deploy:build": "run-s build",
    "deploy:prepare-lockfile": "tsx bin/prepare-lockfile.ts",
    "deploy:deploy": "tsx bin/deploy.ts",
    "deploy:restore-lockfile": "tsx bin/restore-lockfile.ts",
    "deploy:unbork-pnpm": "pnpm i -rf",
    "start": "node build/index.js"
  },
  "dependencies": {
    "@google-cloud/connect-firestore": "^3.0.0",
    "@google-cloud/logging-winston": "^6.0.0",
    "@google-cloud/storage": "^7.13.0",
    "@googleapis/drive": "^8.14.0",
    "@groton/knowledgebase.api": "workspace:*",
    "@groton/knowledgebase.config": "workspace:*",
    "@groton/knowledgebase.openid": "workspace:*",
    "express": "^4.21.1",
    "express-session": "^1.18.1",
    "google-auth-library": "^9.14.2"
  },
  "devDependencies": {
    "@battis/partly-gcloudy": "^0.4.5",
    "@battis/qui-cli": "^0.4.2",
    "@battis/webpack": "^1.1.1",
    "@csstools/css-parser-algorithms": "^3.0.2",
    "@csstools/css-tokenizer": "^3.0.2",
    "@google-cloud/firestore": "^7.10.0",
    "@pnpm/logger": "^5.2.0",
    "@pnpm/make-dedicated-lockfile": "^0.6.18",
    "@tsconfig/recommended": "^1.0.7",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/node": "^20.16.11",
    "acorn": "^8.12.1",
    "add-peer-dependencies": "^0.1.6",
    "app-root-path": "^3.1.0",
    "browserslist": "^4.24.0",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.0",
    "dotenv-webpack": "^8.1.0",
    "favicons": "^7.2.0",
    "favicons-webpack-plugin": "^6.0.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.6.0",
    "mini-css-extract-plugin": "^2.9.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.47",
    "postcss-loader": "^8.1.1",
    "postcss-preset-env": "^10.0.7",
    "postcss-selector-parser": "^6.1.2",
    "raw-loader": "^4.0.2",
    "sass": "^1.79.5",
    "sass-loader": "^16.0.2",
    "style-loader": "^4.0.0",
    "terser-webpack-plugin": "^5.3.10",
    "ts-loader": "^9.5.1",
    "tslib": "^2.7.0",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3",
    "url-loader": "^4.1.1",
    "webpack": "5.95.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.1.0",
    "webpack-node-externals": "^3.0.0",
    "winston": "^3.15.0"
  }
}
