{
  "homepage": "https://github.com/groton-school/knowledgebase#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/groton-school/knowledgebase.git"
  },
  "author": {
    "name": "Seth Battis",
    "email": "sbattis@groton.org"
  },
  "type": "module",
  "directories": {
    "bin": "./bin"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "setup": "run-s setup:*",
    "setup:gcp": "./scripts/setup",
    "setup:deploy": "run-s deploy",
    "setup:log": "run-s setup:log:*",
    "setup:log:venv": "node -e \"if (! require('fs').existsSync('.grpcio')) { process.exit(1)}\" || python3 -m venv .grpcio",
    "setup:log:grpcio": "python3 -m pip install -r requirements.txt",
    "build": "lerna run build",
    "deploy": "run-s deploy:*",
    "deploy:build": "run-s build",
    "deploy:index": "pnpm -F @groton/knowledgebase.indexer run deploy",
    "deploy:router": "pnpm -F @groton/knowledgebase.router run deploy",
    "sync": "run-s deploy",
    "log": "source .env; PATH=\".grpcio/bin:$PATH\" VIRTUAL_ENV=\".grpcio/bin/python\" gcloud alpha logging tail --project=$PROJECT"
  },
  "devDependencies": {
    "@battis/eslint-config": "^2.0.9",
    "@battis/partly-gcloudy": "^3.2.0",
    "@battis/prettier-config": "^1.7.3",
    "@commitlint/config-conventional": "^20.3.1",
    "@commitlint/config-pnpm-scopes": "^20.1.0",
    "@groton/knowledgebase.config": "workspace:*",
    "@groton/knowledgebase.indexer": "workspace:*",
    "@inquirer/prompts": "^8.1.0",
    "@qui-cli/colors": "^3.2.1",
    "@qui-cli/core": "^5.0.2",
    "@qui-cli/env-1password": "^1.2.5",
    "@qui-cli/log": "^4.0.2",
    "@qui-cli/root": "^3.0.6",
    "@qui-cli/shell": "^3.1.1",
    "@qui-cli/validators": "^3.0.3",
    "@tsconfig/node24": "^24.0.3",
    "add-peer-dependencies": "^0.1.13",
    "del-cli": "^6.0.0",
    "lerna": "^8.2.4",
    "monorepo-package-paths": "^0.4.3",
    "npm-run-all": "^4.1.5",
    "nx": "^21.6.10"
  },
  "packageManager": "pnpm@10.28.0+sha512.05df71d1421f21399e053fde567cea34d446fa02c76571441bfc1c7956e98e363088982d940465fd34480d4d90a0668bc12362f8aa88000a64e83d0b0e47be48"
}
